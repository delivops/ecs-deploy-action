{
  "containerDefinitions": [
    {
      "command": [],
      "entryPoint": [],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "API_VERSION",
          "value": "1"
        },
        {
          "name": "TIMEOUT",
          "value": "30"
        }
      ],
      "essential": true,
      "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/test-app:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/test-cluster/test-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "/default"
        }
      },
      "name": "app",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "default",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:database-credentials-abc123:DB_USERNAME::"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:database-credentials-abc123:DB_PASSWORD::"
        },
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:database-credentials-abc123:DB_HOST::"
        },
        {
          "name": "EXTERNAL_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:api-keys-def456:EXTERNAL_API_KEY::"
        },
        {
          "name": "WEBHOOK_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:api-keys-def456:WEBHOOK_SECRET::"
        },
        {
          "name": "SSL_CERT",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:certificates-ghi789:SSL_CERT::"
        },
        {
          "name": "SSL_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:123456789012:secret:certificates-ghi789:SSL_KEY::"
        }
      ]
    }
  ],
  "cpu": "512",
  "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
  "family": "test-cluster_test-service",
  "memory": "1024",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "taskRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole"
}